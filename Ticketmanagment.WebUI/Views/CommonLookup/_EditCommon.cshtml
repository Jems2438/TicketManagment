@model TicketManagment.Core.Models.CommonLookup
@Html.AntiForgeryToken()
@if (ViewBag.Message != null)
{
    <span class="text-success">@ViewBag.Message</span>
}
<span class="alert-danger">
    @Html.ValidationSummary()
</span>

<div class="form-group">
    <label>LookupName:</label>

    @Html.EditorFor(model => model.LookupName, new { htmlAttributes = new { @class = "form-control" } })
    @Html.ValidationMessageFor(model => model.LookupName, "", new { @class = "text-danger" })

</div>

<div class="form-group">
    <label>LookupKey:</label>
